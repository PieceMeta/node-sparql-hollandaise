    "docs": "./node_modules/.bin/yuidoc -o ./doc/ -t ./node_modules/yuidoc-bootstrap-theme -H ./node_modules/yuidoc-bootstrap-theme/helpers/helpers.js ./src/sparql/"
  },
  "author": "Anton Koch <piecemeta@gmail.com>",
  "homepage": "https://github.com/PieceMeta/node-sparql-hollandaise",
