  "peerDependencies": {
    "bluebird": "^3.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "babelify": "^7.2.0",
    "bluebird": "^3.5.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "eslint": "^4.2.0",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-header": "^1.7.1",
    "gulp-minify": "0.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "stability": "^1.0.7",
